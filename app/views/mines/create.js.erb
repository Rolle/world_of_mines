var m = L.marker(
	L.latLng(<%= @mine.latitude.to_s %>, <%= @mine.longitude.to_s %>),
	{ title: '<%=@mine.name + " ("+sort_of_text(@mine.sort)+")" %>', 
	icon: redMarker });
